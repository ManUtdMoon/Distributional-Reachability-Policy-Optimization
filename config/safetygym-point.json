{
  "env_name": "safetygym-point",
  "env_cfg": {
    "robot_type": "point"
  },
  "alg_cfg": {
    "sac_cfg": {
      "target_entropy": -2.0,
      "constraint_threshold": 0.0,
      "mlp_multiplier": false,
      "penalty_lb": -1.0,
      "penalty_ub": 100.0,
      "constrained_fcn": "cost",
      "enable_csc": true,
      "csc_weight_coefficient": 0.05,
      "actor_lr": 8e-5,
      "actor_lr_end": 4e-5,
      "multiplier_lr": 3e-4
    },
    "steps_per_epoch": 1000,
    "model_update_period": 250,
    "model_initial_steps": 10000,
    "model_steps": 1000,
    "buffer_min": 5000,
    "buffer_max": 500000,
    "reward_scale": 50.0,
    "alive_bonus": 0.0,
    "real_fraction": 1.0,
    "horizon": 1
  },
  "epochs": 100
}